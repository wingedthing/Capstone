{
    "initialize.py": {
        "runtime": 0,
        "use_testbed": false,
        "test_old": false,
        "old_data": {
            "type": "json",
            "client_id": "old_data_handler",
            "topic_pub": "Data/raw",
            "path": "test.log",
            "hz": 25
        },
        "should_log_output": false,
        "log_data": {
            "client_id": "data_log_handler",
            "topic_sub": "Data/location",
