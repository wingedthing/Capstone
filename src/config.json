    "logger_process.py": {
        "config": {
            "version": 1,
            "formatters": {
                "detailed": {
                    "class": "logging.Formatter",
                    "format": "%(asctime)s %(levelname)-8s %(processName)-20s %(message)s"
                }
            },
            "handlers": {
                "console": {
                    "class": "logging.StreamHandler",
                    "level": "INFO",
                    "formatter": "detailed"
                },
                "file": {
                    "class": "logging.FileHandler",
                    "filename": "app_log.log",
                    "mode": "w",
                    "formatter": "detailed"
                }
            },
            "root": {
                "level": "DEBUG",
                "handlers": ["console", "file"]
            }
        }
    },
